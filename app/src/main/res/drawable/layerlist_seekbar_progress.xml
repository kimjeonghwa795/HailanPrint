<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape
            android:shape="line">
            <stroke
                android:width="3dp"
                android:color="@color/image_overlay"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape
                android:shape="line">
                <stroke
                    android:width="3dp"
                    android:color="@color/color_f88e11"/>
                <corners android:radius="10dp"/>
            </shape>
        </clip>
    </item>

</layer-list>
